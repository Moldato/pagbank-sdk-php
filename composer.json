{
    "name": "moldato/pagbank-sdk-php",
    "description": "A library to support of the pagbank api v4",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Moldato\\PGB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moldato\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Moldato Technological Solutions",
            "email": "contato@moldato.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "scripts": {
        "test": "@composer dump-autoload -o && php -d xdebug.remote_autostart=Off -d xdebug.remote_enable=Off vendor/bin/phpunit --no-coverage",
        "test:coverage": "@composer dump-autoload -o && php -d xdebug.remote_autostart=Off -d xdebug.remote_enable=Off vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "9"
    }
}
{
    "name": "moldato/pagbank-sdk-php",
    "description": "A library to support of the pagbank api",
    "version": "0.1.0",
    "type": "library",
    "license": "later",
    "autoload": {
        "psr-4": {
            "Moldato\\PagBankSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moldato\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Moldato Technological Solutions",
            "email": "contato@moldato.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "platform": {
            "php": "7.3"
        },
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "@php -d xdebug.remote_autostart=Off -d xdebug.remote_enable=Off vendor/bin/phpunit --no-coverage",
        "test:coverage": "@php -d xdebug.remote_autostart=Off -d xdebug.remote_enable=Off vendor/bin/phpunit"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9"
    }
}